    "pre-commit-lint"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "eslint --rule 'prettier/prettier: [\"error\", {\"singleQuote\": true}]'",
      "git add"
    ]
  }
